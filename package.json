{
  "name": "@jhqn/stylelint-config",
  "version": "0.1.0",
  "packageManager": "pnpm@7.13.5",
  "description": "jhqn's stylelint config",
  "author": "jade-gjz",
  "license": "MIT",
  "keywords": [],
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "release": "bumpp package.json --commit --push --tag && pnpm publish --access public",
    "postpublish": "conventional-changelog -p angular -i CHANGELOG.md -s -r 2 && git add CHANGELOG.md && git commit -am \"chore(docs): update CAHNGELOG.md\" && git push"
  },
  "peerDependencies": {
    "stylelint": "^14.9.1"
  },
  "dependencies": {
    "postcss-html": "^1.5.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-css-modules": "^1.2.2",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-no-unsupported-browser-features": "^6.0.0",
    "stylelint-scss": "^4.3.0"
  },
  "devDependencies": {
    "@jhqn/eslint-config-basic": "^0.1.0",
    "bumpp": "^8.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.18.0",
    "stylelint": "^14.11.0",
    "typescript": "^4.8.2"
  }
}

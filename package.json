{
  "name": "@jhqn/stylelint-config",
  "type": "module",
  "version": "0.8.0",
  "packageManager": "pnpm@10.28.2",
  "description": "jhqn's stylelint config",
  "author": "jade-gjz",
  "license": "MIT",
  "homepage": "https://github.com/jaderd-jh/stylelint-config/tree/main#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaderd-jh/stylelint-config"
  },
  "bugs": {
    "url": "https://github.com/jaderd-jh/stylelint-config/issues"
  },
  "keywords": [],
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "stylistic-rules.js"
  ],
  "scripts": {
    "lint": "eslint --cache .",
    "stylelint": "stylelint fixtures/**/*.{css,scss,vue} -q",
    "update": "pnpm up -i --latest",
    "release": "bumpp"
  },
  "peerDependencies": {
    "stylelint": "^17.0.0"
  },
  "dependencies": {
    "@stylistic/stylelint-plugin": "^5.0.1",
    "postcss-html": "^1.8.1",
    "stylelint-config-css-modules": "^4.6.0",
    "stylelint-config-recess-order": "^7.6.0",
    "stylelint-config-recommended": "^18.0.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^40.0.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^3.0.0",
    "stylelint-no-unsupported-browser-features": "^8.0.5",
    "stylelint-scss": "^7.0.0"
  },
  "devDependencies": {
    "@jhqn/eslint-config": "^0.11.1",
    "bumpp": "^10.4.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.7.0",
    "sass": "^1.97.3",
    "stylelint": "^17.1.0",
    "typescript": "^5.9.3",
    "vue": "^3.5.27"
  },
  "pnpm": {
    "overrides": {
      "isarray": "npm:@nolyfill/isarray@^1",
      "stylelint-no-unsupported-browser-features>stylelint": "^17.1.0",
      "safe-buffer": "npm:@nolyfill/safe-buffer@^1"
    }
  }
}

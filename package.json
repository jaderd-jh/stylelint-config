{
  "name": "@jhqn/stylelint-config",
  "type": "module",
  "version": "0.6.11",
  "packageManager": "pnpm@9.5.0",
  "description": "jhqn's stylelint config",
  "author": "jade-gjz",
  "license": "MIT",
  "homepage": "https://github.com/jaderd-jh/stylelint-config/tree/main#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaderd-jh/stylelint-config"
  },
  "bugs": {
    "url": "https://github.com/jaderd-jh/stylelint-config/issues"
  },
  "keywords": [],
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "stylistic-rules.js"
  ],
  "scripts": {
    "lint": "eslint --cache .",
    "stylelint": "stylelint fixtures/**/*.{css,scss,vue} -q",
    "update": "pnpm up -i --latest",
    "release": "bumpp"
  },
  "peerDependencies": {
    "stylelint": "^16.0.2"
  },
  "dependencies": {
    "@stylistic/stylelint-plugin": "^2.1.2",
    "postcss-html": "^1.7.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-no-unsupported-browser-features": "^8.0.1",
    "stylelint-scss": "^6.3.0"
  },
  "devDependencies": {
    "@jhqn/eslint-config": "^0.7.3",
    "bumpp": "^9.4.1",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.3.0",
    "sass": "^1.77.2",
    "stylelint": "^16.5.0",
    "typescript": "^5.4.5",
    "vue": "^3.4.27"
  },
  "pnpm": {
    "overrides": {
      "array-includes": "npm:@nolyfill/array-includes@latest",
      "array.prototype.findlastindex": "npm:@nolyfill/array.prototype.findlastindex@latest",
      "array.prototype.flat": "npm:@nolyfill/array.prototype.flat@latest",
      "array.prototype.flatmap": "npm:@nolyfill/array.prototype.flatmap@latest",
      "es-iterator-helpers": "npm:@nolyfill/es-iterator-helpers@latest",
      "hasown": "npm:@nolyfill/hasown@latest",
      "isarray": "npm:@nolyfill/isarray@latest",
      "object.assign": "npm:@nolyfill/object.assign@latest",
      "object.entries": "npm:@nolyfill/object.entries@latest",
      "object.fromentries": "npm:@nolyfill/object.fromentries@latest",
      "object.groupby": "npm:@nolyfill/object.groupby@latest",
      "object.values": "npm:@nolyfill/object.values@latest"
    }
  }
}

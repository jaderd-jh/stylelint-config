{
  "name": "@jhqn/stylelint-config",
  "type": "module",
  "version": "0.7.0",
  "packageManager": "pnpm@10.15.0",
  "description": "jhqn's stylelint config",
  "author": "jade-gjz",
  "license": "MIT",
  "homepage": "https://github.com/jaderd-jh/stylelint-config/tree/main#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaderd-jh/stylelint-config"
  },
  "bugs": {
    "url": "https://github.com/jaderd-jh/stylelint-config/issues"
  },
  "keywords": [],
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "stylistic-rules.js"
  ],
  "scripts": {
    "lint": "eslint --cache .",
    "stylelint": "stylelint fixtures/**/*.{css,scss,vue} -q",
    "update": "pnpm up -i --latest",
    "release": "bumpp"
  },
  "peerDependencies": {
    "stylelint": "^16.22.0"
  },
  "dependencies": {
    "@stylistic/stylelint-plugin": "^4.0.0",
    "postcss-html": "^1.8.0",
    "stylelint-config-css-modules": "^4.5.1",
    "stylelint-config-recess-order": "^7.1.0",
    "stylelint-config-recommended": "^16.0.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-no-unsupported-browser-features": "^8.0.4",
    "stylelint-scss": "^6.12.1"
  },
  "devDependencies": {
    "@jhqn/eslint-config": "^0.9.4",
    "bumpp": "^10.2.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.31.0",
    "sass": "^1.89.2",
    "stylelint": "^16.22.0",
    "typescript": "^5.8.3",
    "vue": "^3.5.18"
  },
  "pnpm": {
    "overrides": {
      "isarray": "npm:@nolyfill/isarray@^1",
      "is-core-module": "npm:@nolyfill/is-core-module@^1",
      "safe-buffer": "npm:@nolyfill/safe-buffer@^1"
    }
  }
}

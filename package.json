{
  "name": "@jhqn/stylelint-config",
  "version": "0.3.0",
  "description": "jhqn's stylelint config",
  "author": "jade-gjz",
  "license": "MIT",
  "keywords": [],
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "stylelint": "stylelint fixtures/**/*.{css,scss,vue}",
    "release": "bumpp package.json --commit --push --tag && pnpm publish --access public",
    "postpublish": "conventional-changelog -p angular -i CHANGELOG.md -s -r 2 && git add CHANGELOG.md && git commit -am \"chore(docs): update CAHNGELOG.md\" && git push"
  },
  "peerDependencies": {
    "stylelint": "^14.9.1 || ^15.0.0"
  },
  "dependencies": {
    "postcss-html": "^1.5.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended": "^11.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^31.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-css-modules": "^1.2.2",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-no-unsupported-browser-features": "^6.1.0",
    "stylelint-scss": "^4.6.0",
    "stylelint-stylistic": "^0.3.1"
  },
  "devDependencies": {
    "@jhqn/eslint-config-vue": "^0.3.2",
    "bumpp": "^9.1.0",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.37.0",
    "sass": "^1.60.0",
    "stylelint": "^15.4.0",
    "typescript": "^4.9.5",
    "vue": "^3.2.47"
  }
}
